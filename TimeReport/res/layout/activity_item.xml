<?xml version="1.0" encoding="utf-8"?>

<LinearLayout android:id="@+id/LinearLayout01"
	android:layout_width="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="horizontal" android:layout_height="wrap_content">



	<TextView android:layout_height="wrap_content" android:id="@+id/ActName"
		android:text="@string/empty_string" android:textSize="24sp"
		android:layout_weight="6" android:layout_width="0px"></TextView>
	<TextView android:layout_below="@id/ActName"
		android:layout_height="wrap_content"
		android:id="@+id/ActNumber" android:text="@string/empty_string" android:layout_weight="3" android:layout_width="0px" android:textSize="24sp"></TextView>



	<CheckBox android:layout_height="wrap_content" android:id="@+id/StatusCheckBox"
		android:focusable="false" android:focusableInTouchMode="false"
		android:layout_width="0px" android:layout_weight="1"></CheckBox>
</LinearLayout>
