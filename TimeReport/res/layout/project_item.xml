<?xml version="1.0" encoding="utf-8"?>

<LinearLayout android:id="@+id/LinearLayout01"
	android:layout_width="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="horizontal" android:layout_height="wrap_content">



	<TextView android:layout_height="wrap_content" android:id="@+id/ProjectName" android:text="@string/empty_string" android:textSize="24sp" android:layout_weight="6" android:layout_width="0px"></TextView><LinearLayout android:id="@+id/LinearLayout02"
		android:layout_height="wrap_content"
		android:orientation="vertical" android:layout_weight="3" android:layout_width="0px">
		
		<TextView android:layout_below="@id/ProjectName"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:id="@+id/ProjectNumber" android:text="@string/empty_string"></TextView>
		<TextView android:text="@string/empty_string" android:id="@+id/ProjectRate"
			android:layout_below="@id/ProjectNumber" android:layout_width="wrap_content"
			android:layout_height="wrap_content"></TextView>
	
</LinearLayout>
	


<CheckBox android:layout_height="wrap_content" android:id="@+id/StatusCheckBox" android:focusable="false" android:focusableInTouchMode="false" android:layout_width="0px" android:layout_weight="1"></CheckBox>
</LinearLayout>
